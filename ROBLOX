-- StarterGui â†’ LocalScript "NeonBeachWheel"
local TweenService = game:GetService("TweenService")
local SoundService = game:GetService("SoundService")
local Lighting = game:GetService("Lighting")
local plr = script.Parent.Parent.Parent
local spinWheel = plr:WaitForChild("SpinWheelUI"):WaitForChild("SPIN"):WaitForChild("SpinWheel")
local openButton = script.Parent

-- ğŸŒˆ NEON BEACH COLORS
local NEON_PALETTE = {
    NeonOcean = Color3.fromRGB(0, 255, 255),      -- Cyan glow
    NeonSunset = Color3.fromRGB(255, 100, 150),   -- Pink sunset
    NeonPalm = Color3.fromRGB(0, 255, 127),       -- Green palm
    NeonSand = Color3.fromRGB(255, 235, 150),     -- Golden sand
    NeonWave = Color3.fromRGB(100, 255, 255)      -- Wave cyan
}

local originalSize = spinWheel.Size
local originalPos = spinWheel.Position

-- ğŸµ NEON BEACH SOUNDS
local sounds = {
    open = Instance.new("Sound", script); open.SoundId = "rbxassetid://9044294353"; open.Volume = 0.6, -- Tropical whoosh
    glow = Instance.new("Sound", script); glow.SoundId = "rbxassetid://131961136"; glow.Volume = 0.3,  -- Wave hum
}

openButton.Activated:Connect(function()
    if not spinWheel.Visible then
        -- âš¡ NEON EXPLOSION START
        sounds.open:Play()
        
        -- ğŸŒŠ Reset + NEON setup
        spinWheel.Size = UDim2.new(0, 0, 0, 0)
        spinWheel.Position = UDim2.new(0.5, 0, 0.5, 0)
        spinWheel.AnchorPoint = Vector2.new(0.5, 0.5)
        spinWheel.BackgroundTransparency = 1
        spinWheel.BorderSizePixel = 0
        spinWheel.Visible = true
        
        -- ğŸ”¥ NEON GLOW FRAME (UIStroke + UIGradient)
        local neonStroke = Instance.new("UIStroke")
        neonStroke.Color = NEON_PALETTE.NeonOcean
        neonStroke.Thickness = 8
        neonStroke.Transparency = 0.3
        neonStroke.Parent = spinWheel
        
        local gradient = Instance.new("UIGradient")
        gradient.Color = ColorSequence.new{
            ColorSequenceKeypoint.new(0, NEON_PALETTE.NeonOcean),
            ColorSequenceKeypoint.new(0.5, NEON_PALETTE.NeonSunset),
            ColorSequenceKeypoint.new(1, NEON_PALETTE.NeonPalm)
        }
        gradient.Rotation = 45
        gradient.Parent = spinWheel
        
        -- ğŸ’¦ NEON WAVE PARTICLES
        local particles = Instance.new("ParticleEmitter")
        particles.Texture = "rbxassetid://241650934"
        particles.Color = ColorSequence.new(NEON_PALETTE.NeonWave)
        particles.Lifetime = NumberRange.new(0.8, 1.5)
        particles.Rate = 100
        particles.Speed = NumberRange.new(10, 20)
        particles.SpreadAngle = Vector2.new(60, 60)
        particles.Acceleration = Vector3.new(0, -50, 0)
        particles.Parent = spinWheel
        
        -- ğŸŒŸ MULTI-STAGE NEON ANIMATION
        local stage1 = TweenService:Create(spinWheel, 
            TweenInfo.new(0.3, Enum.EasingStyle.Quart, Enum.EasingDirection.Out),
            {Size = UDim2.new(0.6, 0, 0.6, 0), BackgroundTransparency = 0.7}
        )
        
        local stage2 = TweenService:Create(spinWheel, 
            TweenInfo.new(0.4, Enum.EasingStyle.Back, Enum.EasingDirection.Out),
            {
                Size = originalSize,
                Position = originalPos,
                BackgroundTransparency = 0.2,
                Rotation = 720  -- DOUBLE SPIN!
            }
        )
        
        -- ğŸ¬ SEQUENCE
        stage1:Play()
        stage1.Completed:Connect(function()
            sounds.glow:Play()
            stage2:Play()
        end)
        
        -- âœ¨ INFINITE NEON PULSE
        spawn(function()
            while spinWheel.Parent do
                TweenService:Create(neonStroke, TweenInfo.new(1.2, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, -1, true), 
                    {Thickness = 12, Transparency = 0.1}):Play()
                    
                TweenService:Create(gradient, TweenInfo.new(3, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut, -1), 
                    {Rotation = 360}):Play()
            end
        end)
        
        -- ğŸ’¨ Particles fade
        game:GetService("Debris"):AddItem(particles, 3)
    end
end)

-- ğŸŒ™ NEON LIGHTING BOOST (optional)
local neonLight = Instance.new("PointLight")
neonLight.Color = NEON_PALETTE.NeonOcean
neonLight.Brightness = 2
neonLight.Range = 20
neonLight.Parent = spinWheel
