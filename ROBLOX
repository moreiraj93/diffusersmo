-- On suppose que 'PlayerData' est un ModuleScript ou Folder sous ce script
local plrData = script:WaitForChild("PlayerData")

local function saveData(player)
print("DataStore Sauvegarde des donnÃ©es pour : "..player.Name)

local data = {}
for _, folder in pairs(player:GetChildren()) do
if plrData:FindFirstChild(folder.Name) then
if plrDatafolder.Name:GetAttribute("SaveChildren") == true then
datafolder.Name = {}
if plrDatafolder.Name:GetAttribute("SaveChildrenValues") == true then
for _, child in pairs(folder:GetChildren()) do
if not child:GetAttribute("DoNotSaveValue") then
table.insert(datafolder.Name, {child.Name, child.Value, child.ClassName})
end
end
else
for _, child in pairs(folder:GetChildren()) do
if not child:GetAttribute("DoNotSaveValue") then
table.insert(datafolder.Name, {child.Name, child.ClassName})
end
end
end
end
end
end

local success, errorMsg = pcall(function()
dataStore:SetAsync(player.UserId .. "Key", data)
end)

if success then
print("DataStore DonnÃ©es sauvegardÃ©es avec succÃ¨s pour : "..player.Name)
else
warn("DataStore ProblÃ¨me de sauvegarde pour "..player.Name.." : "..errorMsg)
end
end

local function loadData(player)
print("DataStore Chargement des donnÃ©es pour : "..player.Name)

-- Clone la structure
for _, v in pairs(plrData:GetChildren()) do
v:Clone().Parent = player
end

-- Au lieu de "workspace.MainFolder", on vise:
