data = dataStore:GetAsync(player.UserId .. "Key")
end)

if errorMsg then
warn("DataStore Erreur en chargeant les donnÃ©es : "..errorMsg)
player:Kick("Impossible de charger vos donnÃ©es, veuillez rÃ©essayer.")
return
end

if data then
print("DataStore DonnÃ©es trouvÃ©es pour : "..player.Name..", restauration...")
for folderName, tbl in pairs(data) do
if #tbl > 0 then
for _, entry in pairs(tbl) do
local baseFolder = player:FindFirstChild(folderName)
if baseFolder then
local valueObj = baseFolder:FindFirstChild(entry1)
if not valueObj then
-- CrÃ©e l'objet si nÃ©cessaire
if entry3 == nil then
valueObj = Instance.new(tostring(entry2))
else
valueObj = Instance.new(tostring(entry3))
end
valueObj.Name = entry1
valueObj.Parent = baseFolder
end
