-- StarterGui â†’ BeachWheelClient (LocalScript)
local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local TweenService = game:GetService("TweenService")
local player = Players.LocalPlayer

-- Wait for remotes
local remotes = ReplicatedStorage:WaitForChild("BeachWheelRemotes")
local spinRemote = remotes:WaitForChild("SpinWheel")
local updateCoinsRemote = remotes:WaitForChild("UpdateBeachCoins")

-- ğŸ¨ UI Elements (Update paths to your UI)
local playerGui = player:WaitForChild("PlayerGui")
local spinWheelUI = playerGui:WaitForChild("SpinWheelUI")  -- Your wheel UI
local spinButton = spinWheelUI:WaitForChild("SPIN"):WaitForChild("SpinButton")  -- Your spin btn
local spinsLabel = spinWheelUI:WaitForChild("SPINS_LABEL")  -- TextLabel
local coinsLabel = spinWheelUI:WaitForChild("COINS_LABEL")  -- TextLabel
local resultLabel = spinWheelUI:WaitForChild("RESULT_LABEL") -- Reward text

-- ğŸ”„ Update UI from leaderstats
local function updateUI()
    local leaderstats = player:FindFirstChild("leaderstats")
    if leaderstats then
        spinsLabel.Text = "Spins: " .. leaderstats:FindFirstChild("Spins").Value
        coinsLabel.Text = "ğŸ–ï¸ " .. leaderstats:FindFirstChild("BeachCoins").Value
    end
end

-- ğŸ° SPIN BUTTON
spinButton.Activated:Connect(function()
    spinRemote:FireServer()
end)

-- ğŸ“¡ SERVER RESPONSES
spinRemote.OnClientEvent:Connect(function(result)
    if result then
        resultLabel.Text = result.reward .. " (+$" .. result.coins .. ")"
        resultLabel.Visible = true
        
        -- ğŸ‰ Animation
        resultLabel:TweenSize(UDim2.new(1, 0, 1, 0), "Out", "Back", 0.3)
        wait(2)
        resultLabel:TweenSize(UDim2.new(0, 0, 0, 0), "In", "Quad", 0.5)
        
        updateUI()
    end
end)

-- ğŸ”„ Update on leaderstats change
player.ChildAdded:Connect(function(child)
    if child.Name == "leaderstats" then
        updateUI()
        child.ChildAdded:Connect(updateUI)
        child.ChildRemoved:Connect(updateUI)
    end
end)

print("ğŸ–ï¸ CLIENT READY! Spin away!")
