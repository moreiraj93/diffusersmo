-- ServerScriptService â†’ BeachWheelDataStore (Script)
local DataStoreService = game:GetService("DataStoreService")
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")

local DataStore = DataStoreService:GetDataStore("BeachWheel_v3")
local PlayersData = {}  -- Cache

-- ğŸŒŸ Player Data Template
local function createLeaderstats(player)
    local leaderstats = Instance.new("Folder")
    leaderstats.Name = "leaderstats"
    leaderstats.Parent = player
    
    -- ğŸ–ï¸ BEACH STATS
    local spins = Instance.new("IntValue", leaderstats)
    spins.Name = "Spins"
    
    local coins = Instance.new("IntValue", leaderstats)
    coins.Name = "BeachCoins"
    
    local wins = Instance.new("IntValue", leaderstats)
    wins.Name = "Wins"
    
    local streak = Instance.new("IntValue", leaderstats)
    streak.Name = "Streak"
    
    return leaderstats
end

-- ğŸ’¾ SAVE DATA
local function savePlayerData(player)
    if not PlayersData[player.UserId] then return end
    
    local data = {
        Spins = player.leaderstats.Spins.Value,
        BeachCoins = player.leaderstats.BeachCoins.Value,
        Wins = player.leaderstats.Wins.Value,
        Streak = player.leaderstats.Streak.Value,
        LastSave = os.time(),
        Version = 3
    }
    
    local success, err = pcall(function()
        DataStore:SetAsync("Player_"..player.UserId, data)
    end)
    
    if success then
        print("âœ… SAVED:", player.Name, data.Spins, "spins")
    else
        warn("âŒ SAVE ERROR:", player.Name, err)
    end
end

-- ğŸ“‚ LOAD DATA
local function loadPlayerData(player)
    local leaderstats = createLeaderstats(player)
    
    local success, data = pcall(function()
        return DataStore:GetAsync("Player_"..player.UserId)
    end)
    
    if success and data then
        -- Apply data
        leaderstats.Spins.Value = data.Spins or 0
        leaderstats.BeachCoins.Value = data.BeachCoins or 100  -- Welcome bonus
        leaderstats.Wins.Value = data.Wins or 0
        leaderstats.Streak.Value = data.Streak or 0
        
        print("ğŸ“‚ LOADED:", player.Name, leaderstats.Spins.Value, "spins")
    else
        -- ğŸ NEW PLAYER BONUS
        leaderstats.Spins.Value = 10
        leaderstats.BeachCoins.Value = 250
        print("ğŸŒŠ NEW PLAYER:", player.Name, "+10 spins FREE!")
    end
    
    PlayersData[player.UserId] = leaderstats
end

-- ğŸ”¥ REMOTES (For client)
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local remotes = Instance.new("Folder")
remotes.Name = "BeachWheelRemotes"
remotes.Parent = ReplicatedStorage

local spinRemote = Instance.new("RemoteEvent")
spinRemote.Name = "SpinWheel"
spinRemote.Parent = remotes

local updateCoinsRemote = Instance.new("RemoteEvent")
updateCoinsRemote.Name = "UpdateBeachCoins"
updateCoinsRemote.Parent = remotes

-- ğŸ° SPIN HANDLER
spinRemote.OnServerEvent:Connect(function(player)
    local spins = PlayersData[player.UserId]
    if not spins or spins.leaderstats.Spins.Value <= 0 then
        return  -- No spins
    end
    
    -- Use 1 spin
    spins.leaderstats.Spins.Value -= 1
    
    -- ğŸ² RANDOM REWARD (Beach themed)
    local rewards = {
        {reward = "Nothing ğŸ–ï¸", coins = 0, chance = 50},
        {reward = "BeachCoin ğŸ’°", coins = 10, chance = 30},
        {reward = "SunShell ğŸŒº", coins = 50, chance = 15},
        {reward = "Golden Surf ğŸ„â€â™‚ï¸", coins = 250, chance = 4.9},
        {reward = "Neon Token âœ¨", coins = 1000, chance = 0.1}
    }
    
    local rand = math.random(1, 100)
    local total = 0
    local result = rewards[1]
    
    for _, reward in ipairs(rewards) do
        total += reward.chance
        if rand <= total then
            result = reward
            break
        end
    end
    
    -- Apply reward
    spins.leaderstats.BeachCoins.Value += result.coins
    if result.coins > 0 then
        spins.leaderstats.Wins.Value += 1
        spins.leaderstats.Streak.Value += 1
    end
    
    -- Send to client
    spinRemote:FireClient(player, {
        reward = result.reward,
        coins = result.coins,
        spinsLeft = spins.leaderstats.Spins.Value
    })
    
    print("ğŸ° SPIN:", player.Name, "â†’", result.reward)
end)

-- ğŸ›¡ï¸ EVENTS
Players.PlayerAdded:Connect(loadPlayerData)
Players.PlayerRemoving:Connect(function(player)
    savePlayerData(player)
    PlayersData[player.UserId] = nil
end)

-- ğŸ’¾ AUTO SAVE (Every 30s)
spawn(function()
    while true do
        wait(30)
        for userId, _ in pairs(PlayersData) do
            local player = Players:GetPlayerByUserId(userId)
            if player then
                savePlayerData(player)
            end
        end
    end
end)

game:BindToClose(function()
    for _, player in pairs(Players:GetPlayers()) do
        savePlayerData(player)
    end
end)

print("ğŸ–ï¸ BEACH WHEEL DATASTORE âœ… READY!")
