-- StarterGui â†’ LocalScript "BeachWheelOpen"
local TweenService = game:GetService("TweenService")
local SoundService = game:GetService("SoundService")
local plr = script.Parent.Parent.Parent  -- PlayerGui
local spinWheel = plr:WaitForChild("SpinWheelUI"):WaitForChild("SPIN"):WaitForChild("SpinWheel")
local openButton = script.Parent

-- ğŸŒŠ Original size + Beach colors
local originalSize = spinWheel.Size
local originalPos = spinWheel.Position

-- ğŸ–ï¸ BEACH OPEN SOUND (Add to ReplicatedStorage)
local openSound = Instance.new("Sound")
openSound.SoundId = "rbxassetid://131961136"  -- Wave crash!
openSound.Volume = 0.5
openSound.Parent = script

openButton.Activated:Connect(function()
    if not spinWheel.Visible then
        -- ğŸ¨ Beach glow effect
        spinWheel.BackgroundColor3 = Color3.fromRGB(255, 228, 181)  -- Beach sand
        spinWheel.BorderColor3 = Color3.fromRGB(0, 191, 255)        -- Ocean blue
        
        -- ğŸŒŠ Wave animation start
        spinWheel.Size = UDim2.new(0, 0, 0, 0)
        spinWheel.Position = UDim2.new(0.5, 0, 0.5, 0)
        spinWheel.AnchorPoint = Vector2.new(0.5, 0.5)
        spinWheel.Visible = true
        
        -- ğŸ’¦ Wave particles (optional)
        local particles = Instance.new("ParticleEmitter")
        particles.Texture = "rbxassetid://241650934"  -- Water splash
        particles.Lifetime = NumberRange.new(0.5, 1.2)
        particles.Rate = 50
        particles.SpreadAngle = Vector2.new(45, 45)
        particles.Parent = spinWheel
        
        -- ğŸµ Beach wave sound
        openSound:Play()
        
        -- ğŸŒŠ Perfect wave tween (0.4s = smoother)
        local tweenInfo = TweenInfo.new(
            0.4, 
            Enum.EasingStyle.Back, 
            Enum.EasingDirection.Out,
            0, -- repeats
            false, -- reverse
            0 -- delay
        )
        
        local goal = {
            Size = originalSize,
            Position = originalPos,
            Rotation = 360  -- Full spin!
        }
        
        local tween = TweenService:Create(spinWheel, tweenInfo, goal)
        tween:Play()
        
        -- âœ¨ Particles fade
        wait(1)
        particles.Enabled = false
        
        -- ğŸ‰ Glow pulse
        local glowTween = TweenService:Create(spinWheel, 
            TweenInfo.new(1.5, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, -1, true),
            {BackgroundTransparency = 0.1}
        )
        glowTween:Play()
    end
end)
