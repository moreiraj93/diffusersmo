-- ServerScriptService â†’ Script "BeachDataStore" 
print("ğŸ–ï¸ [BeachDataStore] Chargement... ğŸŒŠ")

local Players = game:GetService("Players")
local DataStoreService = game:GetService("DataStoreService")
local MessagingService = game:GetService("MessagingService")

-- ğŸ–ï¸ BEACH DATASTORE (unique name!)
local dataStore = DataStoreService:GetDataStore("BeachWheel_SPINS_v2")

-- PlayerData structure (ModuleScript or Folder)
local plrData = script:WaitForChild("PlayerData")

-- ğŸŒŸ AUTO-SAVE EVERY 30s
local AUTO_SAVE_INTERVAL = 30

local function saveData(player)
    print("ğŸ–ï¸ Sauvegarde "..player.Name.." â†’ "..player.leaderstats.Spins.Value.." spins")
    
    local data = {
        Spins = player.leaderstats.Spins.Value,
        BeachCoins = player.leaderstats["Beach Coins"].Value or 0,
        BeachVIP = player.leaderstats["Beach VIP"].Value or false,
        LastPlayed = os.time(),
        Version = 2
    }
    
    local success, errorMsg = pcall(function()
        dataStore:SetAsync("Beach_"..player.UserId, data)
    end)
    
    if success then
        print("âœ… "..player.Name.." sauvegardÃ©!")
    else
        warn("âŒ Erreur save "..player.Name..": "..tostring(errorMsg))
    end
end

local function loadData(player)
    print("ğŸ–ï¸ Chargement "..player.Name)
    
    -- Create leaderstats
    local leaderstats = Instance.new("Folder")
    leaderstats.Name = "leaderstats"
    leaderstats.Parent = player
    
    local spins = Instance.new("IntValue")
    spins.Name = "Spins"
    spins.Value = 0
    spins.Parent = leaderstats
    
    local beachCoins = Instance.new("IntValue")
    beachCoins.Name = "Beach Coins"
    beachCoins.Value = 0
    beachCoins.Parent = leaderstats
    
    local beachVIP = Instance.new("BoolValue")
    beachVIP.Name = "Beach VIP"
    beachVIP.Value = false
    beachVIP.Parent = leaderstats
    
    -- Load from datastore
    local data
    local success, result = pcall(function()
        data = dataStore:GetAsync("Beach_"..player.UserId)
    end)
    
    if success and data then
        print("ğŸ“‚ DonnÃ©es trouvÃ©es:", data)
        spins.Value = data.Spins or 0
        beachCoins.Value = data.BeachCoins or 0
        beachVIP.Value = data.BeachVIP or false
        
        -- Migrate old data
        if data.Version and data.Version < 2 then
            print("ğŸ”„ Migration v1 â†’ v2")
        end
    else
        print("ğŸŒŠ Nouveau joueur:", player.Name)
        -- Give welcome bonus!
        spins.Value = 5
        beachCoins.Value = 100
    end
end

-- ğŸš€ Events
Players.PlayerAdded:Connect(loadData)
Players.PlayerRemoving:Connect(saveData)

-- ğŸ’¾ Auto-save loop
spawn(function()
    while true do
        wait(AUTO_SAVE_INTERVAL)
        for _, player in pairs(Players:GetPlayers()) do
            saveData(player)
        end
        print("ğŸ–ï¸ Auto-save "..#Players:GetPlayers().." joueurs")
    end
end)

-- ğŸ›¡ï¸ Server shutdown
game:BindToClose(function()
    print("ğŸ–ï¸ Fermeture â†’ Sauvegarde d'urgence!")
    for _, player in pairs(Players:GetPlayers()) do
        saveData(player)
    end
    wait(2)
end)

-- ğŸŒŸ Cross-server sync (bonus!)
MessagingService:SubscribeAsync("BeachDataUpdate", function(message)
    local playerId = message.Data.PlayerId
    local update = message.Data.Update
    
    print("ğŸ“¡ Sync "..playerId.." â†’ "..tostring(update.Spins))
end)

print("ğŸ–ï¸ [BeachDataStore] âœ… PRÃŠT! Auto-save toutes les 30s")
